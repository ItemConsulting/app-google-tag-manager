<?xml version="1.0" encoding="UTF-8"?>
<site>
  <form>

    <input type="TextLine" name="googleTagManagerContainerID">
      <label>Container ID</label>
      <occurrences minimum="0" maximum="1"/>
    </input>

    <input  name="shouldRequireCookieConsent" type="CheckBox">
      <label>Require consent from end user?</label>
      <default>checked</default>
      <config>
        <alignment>right</alignment>
      </config>
    </input>

  </form>
  <processors>
    <response-processor name="google-tagmanager" order="10"/>
  </processors>
</site>
